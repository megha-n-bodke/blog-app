<div>
<h1>Add new post</h1>
<%= form_with model: @post, url: user_posts_path do |form|%>
<div>
<%= form.text_field :title, placeholder:"Enter post title", class:"form-control"%>
<%=form.text_field :text, placeholder:"Enter text", class:"form-control"%>
<%=form.submit "Add Post", class:"btn btn-primary", data:{turbo:false}%>
</div>
<%end%>
</div>